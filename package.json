    "@wdio/mocha-framework": "^9.0.6",
    "@wdio/selenium-standalone-service": "^8.14.0",
    "@wdio/spec-reporter": "^9.0.7",
    "@wdio/types": "^9.0.4",
    "@wdio/visual-service": "^6.0.0",
    "chai": "^5.1.1",
    "cucumber": "^6.0.7",
